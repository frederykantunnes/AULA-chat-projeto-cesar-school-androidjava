<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.frederykantunnes.projetochatcesar_androidavancado">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_slack_login"
        android:label="Chat"
        android:roundIcon="@drawable/ic_slack_login"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".feature.Contatos.Contacts"></activity>
        <activity android:name=".Perfil" />
        <activity android:name=".feature.chats.Chats" />
        <activity android:name=".feature.Messages.Messages" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>